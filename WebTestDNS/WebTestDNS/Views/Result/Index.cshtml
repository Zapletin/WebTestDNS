@model IEnumerable<WebTestDNS.Models.CommandModel>

@{
    ViewData["Title"] = "Все команды";
}

<h1 class="text-black-50">Все команды:</h1><br>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
        </tr>
}
    </tbody>
</table>
